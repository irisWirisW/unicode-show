{
  "name": "unicode-show",
  "version": "0.0.4",
  "publisher": "1risW",
  "engines": {
    "vscode": "^1.77.0"
  },
  "displayName": "unicode-show",
  "description": "Show Unicode Characterâš½",
  "icon": "Resources/icon.png",
  "keywords": [
    "vscode",
    "Unicode"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/irisWirisW/unicode-show.git"
  },
  "categories": [
    "Other"
  ],
  "bugs": {
    "url": "https://github.com/irisWirisW/unicode-show/issues"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unicode-show.showUnicode",
        "title": "Show Unicode Character"
      }
    ],
    "keybindings": [
      {
        "command": "unicode-show.showUnicode",
        "key": "ctrl+alt+u",
        "mac": "cmd+alt+u",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus && !editorReadonly",
          "command": "unicode-show.showUnicode",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "unicode-show": {
        "showUnicode": {
          "type": "boolean",
          "default": true,
          "description": "Show Unicode Character"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/vsce": "^3.2.1",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "license": "MIT"
}